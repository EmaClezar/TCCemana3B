<h1>Lista de clientes</h1>
<table class="table">
    <thead>

        <tr>
            <th>Nome</th>
            <th>Email</th>
            <th>Telefone</th>
            <th>Endere√ßo</th>
            <th>CPF</th>
            <th>Valor da Divida</th>

        </tr>
    </thead>
    <tbody>
        <% clientes.forEach(clientes => { %>
            <tr>
              
                <td><%= clientes.nome %></td>
                <td><%= clientes.email %></td>
                <td><%= clientes.telefone %></td>
                <td><%= clientes.endereco %></td>
                <td><%= clientes.cpf %></td>
                <td><%= clientes.valorDivida %></td>
                <td>
                    <a href="/clientes/<%= clientes.id %>" class="btn btn-primary">Vizualizar</a>
                    <a href="/clientes/<%= clientes.id %>/edit" class="btn btn-warning">Editar</a>
                    <form action="/clientes/<%= clientes.id %>?_method=DELETE" method="POST" style="display: inline;">
                        <button type="submit" class="btn btn-danger">Excluir</button>
                    </form>
                </td>
            </tr>
        <% }) %>
    </tbody>
</table>
